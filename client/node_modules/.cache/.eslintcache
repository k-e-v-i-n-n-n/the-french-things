bject(e)} ></input>\n                    <label className=\"modal-label\">Type</label>\n                    <select value={translationObject[\"type\"]} className=\"modal-input\" onChange={(e) => {setTranslationObject({...translationObject, type: e.target.value}); setPostPath(e.target.value)}} >\n                        <option value=\"\" defaultValue>-Select Entry Type-</option>\n                        <option >Expression</option>\n                        <option>Word</option>\n                    </select>\n                    {alert && <p id=\"error-modal\">Please fill in all fields</p>}\n                    <div className=\"modal-button-container\">\n                        <button id=\"modal-save\" onClick={(e) => {save(e)}} >save</button>\n                        <button id=\"modal-cancel\" onClick={cancelState}>cancel</button>\n                    </div>\n                </form>\n            </div>\n        </div>\n    )\n}\n\nexport default Modal","/Users/kevinlocke/Dropbox/Development/code/encodeur/Phase-5-Project-and-Notes/phase-5-repo/client/src/components/CategoryModal.js",[],[],"/Users/kevinlocke/Dropbox/Development/code/encodeur/Phase-5-Project-and-Notes/phase-5-repo/client/src/components/WordStarModal.js",[],[],"/Users/kevinlocke/Dropbox/Development/code/encodeur/Phase-5-Project-and-Notes/phase-5-repo/client/src/pages/Stars.js",[],[],[],"/Users/kevinlocke/Dropbox/Development/code/encodeur/Phase-5-Project-and-Notes/phase-5-repo/client/src/components/WordStarCard.js",["122","123"],[],"import { useContext } from \"react\"\nimport { AppContext } from \"../Context\"\n\nconst WordStarCard = ({word}) => {\n\n  const {user, setUser} = useContext(AppContext)\n\n    return (\n      <div className=\"expression-container\" >\n          <p className=\"target-expression-text\">\n            {word.french}\n          </p>\n          <p className=\"source-expression-text\">\n           {word.english}\n          </p>\n      </div>\n    )\n  }\n  \n  export default Word